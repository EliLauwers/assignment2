library("DBI")
library("readr")
library("jsonlite")
knitr::opts_chunk$set(
echo = TRUE
)
database_creds = read_json("database_creds.json")
database_creds[["drv"]] = RPostgres::Postgres()
con = do.call(
RPostgres::dbConnect,
database_creds
)
printlines = 25
